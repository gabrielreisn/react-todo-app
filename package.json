{
  "name": "react-todo-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.16.2",
    "express-graphql": "^0.6.11",
    "faker": "^4.1.0",
    "graphql": "^0.11.7",
    "graphql-relay": "^0.5.4",
    "material-ui": "^0.20.0",
    "mongoose": "^4.13.6",
    "nodemon": "^1.13.3",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.0.17",
    "styled-components": "^2.2.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "flow": "flow",
    "prettier":
      "prettier --write --single-quote true --trailing-comma all --print-width 120 --bracket-spacing false --jsx-bracket-same-line true",
    "dev": "concurrently \"mongod\" \"npm run start\" \"node server/server.js\"",
    "server": "concurrently \"mongod\"  \"node server/ \"",
    "graphql": "nodemon server/"
  },
  "lint-staged": {
    "*.{js}": ["eslint --fix", "git add", "prettier"]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "concurrently": "^3.5.1",
    "eslint": "^4.12.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "flow-bin": "^0.60.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "prettier": "1.9.1"
  }
}
